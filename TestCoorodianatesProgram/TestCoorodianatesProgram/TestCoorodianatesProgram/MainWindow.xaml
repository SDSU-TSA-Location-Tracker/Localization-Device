<Window x:Class="TestCoorodianatesProgram.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestCoorodianatesProgram"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel Background="DarkGray">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
        </Grid>
        <TextBox Grid.Row="1" Name="CoordinatesTextBox" DockPanel.Dock="Top" FontSize="20" Height="40" TextAlignment="Center" />
    </DockPanel>
</Window>
